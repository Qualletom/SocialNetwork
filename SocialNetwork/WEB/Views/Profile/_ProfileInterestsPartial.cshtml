@model WEB.Models.Profile.InterestsModel

<div class="container__wideBlockInfo">
    <div class="blockInfo__header">
        <span>Интересы</span>
    </div>
    <div class="blockInfo__wideBody">
        @foreach (var property in ViewData.ModelMetadata.Properties)
        {
            if (property.PropertyName != "Id")
            {
                <div class="wideBody__info">
                    <div class="info__label">
                        <span>@property.PropertyName</span>
                    </div>
                    <div class="info__description">
                        <span>
                            @(property.Model ?? "Не указано")
                        </span>

                    </div>

                </div>
            }
        }
    </div>
</div>
